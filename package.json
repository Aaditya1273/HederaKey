{
  "name": "hederakey",
  "version": "2.0.0",
  "description": "Decentralized Physical Infrastructure for Real-World Assets - NFC to Blockchain Bridge",
  "main": "dist",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon -w src --exec \"babel-node src\"",
    "client:dev": "cd client && npm start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel src -s -D -d dist",
    "build:client": "cd client && npm run build",
    "start": "node dist",
    "start:web": "npm run build && npm start",
    "start:ai": "python3 ai/main.py",
    "start:depin": "node src/services/depin-relay-network.js",
    "prestart": "npm run build:server",
    "test": "jest && eslint src",
    "test:watch": "jest --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "debug": "npm run prestart && node --nolazy --inspect-brk=9229 dist",
    "demo": "node scripts/demo.js",
    "demo:nfc": "node scripts/demo-nfc.js",
    "demo:ai": "python3 ai/demo_fraud_detection.py",
    "demo:depin": "node scripts/demo-depin.js",
    "demo:amm": "node scripts/demo-amm.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "setup": "npm install && cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://git.heroku.com/xeth.git"
  },
  "author": "Omodaka9375",
  "license": "MIT",
  "dependencies": {
    "@hashgraph/sdk": "^2.49.2",
    "body-parser": "^1.20.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "scrypt": "^6.0.3",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.5",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "hedera",
    "nfc",
    "blockchain",
    "depin",
    "rwa",
    "tokenization",
    "ai",
    "fraud-detection",
    "zero-knowledge",
    "amm"
  ]
}
